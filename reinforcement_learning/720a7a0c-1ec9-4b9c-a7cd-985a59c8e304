   "cell_type": "code",
   "execution_count": 230,
   "id": "c674c7d3",
   "metadata": {},
   "outputs": [],
   "source": [
    "class FeedforwardNeuralNetwork(torch.nn.Module):\n",
    "    def __init__(self):\n",
    "        super(FeedforwardNeuralNetwork, self).__init__()\n",
    "        self.fc1 = torch.nn.Linear(4, 50)\n",
    "        self.relu = torch.nn.ReLU()\n",
    "        self.fc2 = torch.nn.Linear(50, 3)\n",
    "        self.softmax = torch.nn.Softmax(dim=1)\n",
    "    \n",
    "    def forward(self, x):\n",
    "        output = self.fc1(x)\n",
    "        output = self.relu(output)\n",
    "        output = self.fc2(output)\n",
    "        output = self.softmax(output)\n",
    "        return output"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 231,
   "id": "551cb96d",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = FeedforwardNeuralNetwork()"
   ]
  },
