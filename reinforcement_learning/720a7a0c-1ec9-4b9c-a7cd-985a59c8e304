    "criterion = torch.nn.CrossEntropyLoss()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 238,
   "id": "efd288a6",
   "metadata": {},
   "outputs": [],
   "source": [
    "learning_rate = 0.1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 239,
   "id": "3cff9ee1",
   "metadata": {},
   "outputs": [],
   "source": [
    "num_epochs = 5_000"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 240,
   "id": "4c101002",
   "metadata": {},
   "outputs": [],
   "source": [
    "optimizer = torch.optim.SGD(model.parameters(), lr=learning_rate)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 241,
   "id": "aef757a9",
   "metadata": {},
   "outputs": [],
   "source": [
    "model.train();"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 242,
   "id": "3d307661",
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "100%|| 5000/5000 [01:24<00:00, 59.42it/s]\n"
     ]
    }
   ],
   "source": [
    "loss_history = []\n",
    "accuracy_history = []\n",
    "for epoch in tqdm.trange(num_epochs):\n",
    "    for i, (inputs, targets) in enumerate(data_loader):\n",
    "        optimizer.zero_grad()\n",
    "        y_pred = model(inputs)\n",
    "        loss = criterion(y_pred, targets)\n",
    "        loss.backward()\n",
    "        optimizer.step()\n",
    "        loss_history.append(loss.item())\n",
    "        correct = (torch.argmax(y_pred, dim=1) == targets).type(torch.FloatTensor)\n",
    "        accuracy_history.append(correct.mean())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 243,
   "id": "e79d961c",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 864x432 with 2 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "fig, (ax1, ax2) = plt.subplots(2, figsize=(12, 6), sharex=True)\n",
    "ax1.plot(loss_history)\n",
    "ax1.set_ylabel('loss')\n",
    "ax2.plot(accuracy_history)\n",
    "ax2.set_ylabel('accuracy')\n",
    "ax2.set_xlabel('iteration');"
   ]
  },
