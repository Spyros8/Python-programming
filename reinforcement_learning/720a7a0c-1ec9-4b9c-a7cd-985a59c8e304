   "cell_type": "code",
   "execution_count": 64,
   "id": "1143526a",
   "metadata": {},
   "outputs": [],
   "source": [
    "and_loss_history = []\n",
    "for epoch in range(num_epochs):\n",
    "    for i, (inputs, targets) in enumerate(data_loader):\n",
    "        optimizer.zero_grad()\n",
    "        y_pred = model(inputs)\n",
    "        loss = criterion(y_pred, targets)\n",
    "        loss.backward()\n",
    "        optimizer.step()\n",
    "        and_loss_history.append(loss.item())"
   ]
  },
