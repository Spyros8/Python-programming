   "cell_type": "code",
   "execution_count": 277,
   "id": "cf970f90",
   "metadata": {},
   "outputs": [],
   "source": [
    "class FeedforwardNeuralNetwork(torch.nn.Module):\n",
    "    def __init__(self):\n",
    "        super(FeedforwardNeuralNetwork, self).__init__()\n",
    "        self.fc1 = torch.nn.Linear(6, 50)\n",
    "        self.relu = torch.nn.ReLU()\n",
    "        self.fc2 = torch.nn.Linear(50, 1)\n",
    "        self.linear = torch.nn.Linear(1, 1)\n",
    "    \n",
    "    def forward(self, x):\n",
    "        output = self.fc1(x)\n",
    "        output = self.relu(output)\n",
    "        output = self.fc2(output)\n",
    "        output = self.linear(output)\n",
    "        return output"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 278,
   "id": "d4acf4e3",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = FeedforwardNeuralNetwork()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 279,
   "id": "10b07fb1",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "FeedforwardNeuralNetwork(\n",
       "  (fc1): Linear(in_features=6, out_features=50, bias=True)\n",
       "  (relu): ReLU()\n",
       "  (fc2): Linear(in_features=50, out_features=1, bias=True)\n",
       "  (linear): Linear(in_features=1, out_features=1, bias=True)\n",
       ")"
      ]
     },
     "execution_count": 279,
