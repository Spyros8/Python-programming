   "id": "22968cd7",
   "metadata": {},
   "outputs": [],
   "source": [
    "dataset = torch.utils.data.TensorDataset(tensor_X, tensor_y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 58,
   "id": "4da77b7c",
   "metadata": {},
   "outputs": [],
   "source": [
    "data_loader = torch.utils.data.DataLoader(dataset, batch_size=None,\n",
    "        sampler=torch.utils.data.BatchSampler(torch.utils.data.RandomSampler(dataset), batch_size=4, drop_last=False))"
   ]
  },
  {
