   "cell_type": "markdown",
   "id": "c91f027f",
   "metadata": {},
   "source": [
    "## Feedforward neural network in PyTorch"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 103,
   "id": "596f4cfd",
   "metadata": {},
   "outputs": [],
   "source": [
    "class FeedforwardNeuralNetwork(torch.nn.Module):\n",
    "    def __init__(self):\n",
    "        super(FeedforwardNeuralNetwork, self).__init__()\n",
    "        self.fc1 = torch.nn.Linear(2, 4)\n",
    "        self.relu = torch.nn.ReLU()\n",
    "        self.fc2 = torch.nn.Linear(4, 1)\n",
    "        self.sigmoid = torch.nn.Sigmoid()\n",
    "    \n",
    "    def forward(self, x):\n",
    "        output = self.fc1(x)\n",
    "        output = self.relu(output)\n",
    "        output = self.fc2(output)\n",
    "        output = self.sigmoid(output)\n",
    "        return output"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 104,
   "id": "2f72de8e",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = FeedforwardNeuralNetwork()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 105,
   "id": "c8377812",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "FeedforwardNeuralNetwork(\n",
       "  (fc1): Linear(in_features=2, out_features=4, bias=True)\n",
       "  (relu): ReLU()\n",
       "  (fc2): Linear(in_features=4, out_features=1, bias=True)\n",
       "  (sigmoid): Sigmoid()\n",
       ")"
      ]
     },
     "execution_count": 105,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "model"
   ]
